{
  "name": "canvango-member-area",
  "version": "1.0.0",
  "description": "Canvango Member Area - Consolidated Application",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "npm run build:frontend && npm run build:server && npm run postbuild:server",
    "build:frontend": "vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "postbuild:server": "node -e \"require('fs').writeFileSync('server/dist/package.json', JSON.stringify({type:'module'}, null, 2))\"",
    "build:analyze": "npm run build:frontend && start dist/stats.html",
    "dev": "concurrently -n \"BACKEND,FRONTEND\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run dev:server\" \"vite\"",
    "dev:server": "tsx watch server/src/index.ts",
    "dev:frontend": "vite",
    "start": "node server.js",
    "preview": "vite preview",
    "test": "jest",
    "test:server": "jest --config server/jest.config.js",
    "migrate": "tsx server/src/database/migrate.ts",
    "seed": "tsx server/src/database/seed.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0"
  },
  "keywords": [
    "canvango",
    "member-area",
    "supabase",
    "react"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-brands-svg-icons": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/react-fontawesome": "^3.1.0",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^5.2.2",
    "@marsidev/react-turnstile": "^1.3.1",
    "@octokit/rest": "^20.0.2",
    "@supabase/supabase-js": "^2.81.1",
    "@tanstack/react-query": "^5.90.9",
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "chalk": "^5.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.0.1",
    "helmet": "^8.1.0",
    "inquirer": "^9.2.12",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.553.0",
    "pg": "^8.11.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.0",
    "react-hot-toast": "^2.4.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.9.6",
    "validator": "^13.15.23",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^23.0.1",
    "postcss": "^8.4.49",
    "rollup-plugin-visualizer": "^6.0.5",
    "supertest": "^6.3.3",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^7.2.2",
    "vitest": "^1.1.0"
  }
}
